{
  "name": "@gly-juca/juca-jsx",
  "version": "0.0.3",
  "type": "module",
  "description": "JSX UI Components for apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gly-engine/juca-jsx.git"
  },
  "bugs": "https://github.com/gly-engine/juca-jsx/issues",
  "scripts": {
    "prebuild": "tstl --declaration",
    "build": "gly-cli build-ginga --enterprise --target ncl --cwd dist index.lua --bundler --outdir build/dist --non-relative && cp -r assets build/dist/",
    "postbuild": "cp README.md package.json tsconfig.json .npmignore dist",
    "build:html": "npm run build",
    "postbuild:html": "gly-cli build-html build/dist/game.lua --outdir build/html --aa --enginecdn --fengari --screen 1280x720",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "format:fix": "prettier --write .",
    "format": "prettier --check ."
  },
  "dependencies": {
    "@gamely/gly-cli": "^0.3.10",
    "@gamely/tstl-import": "0.0.3",
    "@gamely/acai-jsx": "^0.0.3"
  },
  "devDependencies": {
    "@gamely/gly-jsx": "^0.3.1",
    "@gamely/gly-types": "^0.2.4",
    "@gamely/tstl-console": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "typescript-eslint": "^8.39.1",
    "typescript-to-lua": "^1.31.2",
    "prettier": "^3.6.2"
  }
}
